{
    "env": {
        "browser": true,
        "node": true,
        "jquery": true,
        "jest": true
    },
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react",
        "plugin:jsx-a11y/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off"
    }
}
